@{
    ViewBag.Title = "User Information";
    Layout = "_Layout";
}

<h2>@ViewBag.Title</h2>

<div>
    <span>User name: </span>@User.Identity.Name
</div>
<div>
    <h5>Claims</h5>
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <td>Claim</td>
            <td>Value</td>
        </tr>
        </thead>
        <tbody>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
        </tbody>
    </table>
</div>
